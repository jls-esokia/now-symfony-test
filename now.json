{
    "version": 2,
    "name": "symfony-test",
    "builds": [{
        "src": "public/*.php",
        "use": "@juicyfx/php@canary",
        "config": {
            "composer": true,
            "v": "7.3"
        }
    }],
    "env": {
        "APP_ENV": "prod",
        "APP_SECRET": "@app-secret",
        "DATABASE_URL": "@database-url",
        "MAILER_URL": "@mailer-url"
    },
    "routes": [{
        "src": "/.*", 
        "dest": "/"
    }]
}
